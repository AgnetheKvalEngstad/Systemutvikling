<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Scene fill="#f5e1b9" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="no.ntnu.idatt1002.demo.expenses.ExpensesController">
   <GridPane style="-fx-background-color: F5E1B9;">
      <columnConstraints>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <children>
         <AnchorPane prefHeight="459.0" prefWidth="587.0">
            <children>
               <PieChart fx:id="pieChart" layoutX="35.0" layoutY="141.0" prefHeight="248.0" prefWidth="229.0" style="-fx-border-color: D8AF7F;" />
               <DatePicker fx:id="fromDate" layoutX="255.0" layoutY="36.0" onAction="#timeInterval" prefHeight="26.0" prefWidth="150.0" promptText="Enter month to view" style="-fx-background-color: D8AF7F;" />
               <DatePicker fx:id="toDate" layoutX="420.0" layoutY="36.0" onAction="#timeInterval" prefHeight="26.0" prefWidth="148.0" promptText="Enter month to view" style="-fx-background-color: D8AF7F;" />
               <CheckBox fx:id="groceriesChecked" layoutX="280.0" layoutY="159.0" mnemonicParsing="false" onAction="#groceriesChecked" prefHeight="22.0" prefWidth="95.0" text="Groceries" textFill="#8e7d69">
                  <font>
                     <Font name="Arial" size="15.0" />
                  </font></CheckBox>
               <CheckBox fx:id="housingChecked" layoutX="280.0" layoutY="197.0" mnemonicParsing="false" onAction="#housingChecked" text="Housing" textFill="#8e7d69">
                  <font>
                     <Font name="Arial" size="15.0" />
                  </font></CheckBox>
               <CheckBox fx:id="fixedExpensesChecked" layoutX="280.0" layoutY="235.0" mnemonicParsing="false" onAction="#fixedExpensesChecked" text="Fixed expenses" textFill="#8e7d69">
                  <font>
                     <Font name="Arial" size="15.0" />
                  </font></CheckBox>
               <CheckBox fx:id="travelChecked" layoutX="280.0" layoutY="272.0" mnemonicParsing="false" onAction="#travelChecked" text="Travel" textFill="#8e7d69">
                  <font>
                     <Font name="Arial" size="15.0" />
                  </font></CheckBox>
               <CheckBox fx:id="entertainmentChecked" layoutX="280.0" layoutY="309.0" mnemonicParsing="false" onAction="#entertainmentChecked" text="Entertainment" textFill="#8e7d69">
                  <font>
                     <Font name="Arial" size="15.0" />
                  </font></CheckBox>
               <CheckBox fx:id="otherChecked" layoutX="280.0" layoutY="344.0" mnemonicParsing="false" onAction="#otherChecked" text="Other" textFill="#8e7d69">
                  <font>
                     <Font name="Arial" size="15.0" />
                  </font></CheckBox>
               <Button fx:id="editCategory" layoutX="439.0" layoutY="181.0" mnemonicParsing="false" onAction="#editCategoryButtonPushed" prefHeight="32.0" prefWidth="118.0" style="-fx-background-color: D8AF7F;" text="Edit categories">
                  <font>
                     <Font name="Arial" size="14.0" />
                  </font></Button>
               <Button fx:id="editExpenses" layoutX="439.0" layoutY="219.0" mnemonicParsing="false" onAction="#editExpensesButtonPushed" prefHeight="32.0" prefWidth="118.0" style="-fx-background-color: D8AF7F;" text="Edit expenses">
                  <font>
                     <Font name="Arial" size="14.0" />
                  </font></Button>
               <Button fx:id="viewIncome" layoutX="439.0" layoutY="256.0" mnemonicParsing="false" onAction="#goToIncome" prefHeight="32.0" prefWidth="118.0" style="-fx-background-color: D8AF7F;" text="View income">
                  <font>
                     <Font name="Arial" size="14.0" />
                  </font></Button>
               <Button fx:id="changeToBarGraph" layoutX="424.0" layoutY="303.0" mnemonicParsing="false" onAction="#changeToBarGraphButtonPushed" prefHeight="32.0" prefWidth="150.0" style="-fx-background-color: D8AF7F;" text="Change to bar graph">
                  <font>
                     <Font name="Arial" size="14.0" />
                  </font></Button>
               <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goHome" style="-fx-background-color: transparent;">
                  <graphic>
                     <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                           <Image url="@logo.png" />
                        </image>
                     </ImageView>
                  </graphic></Button>
               <Label layoutX="41.0" layoutY="96.0" text="Expenses graph" textFill="#8e7d69">
                  <font>
                     <Font name="Arial" size="30.0" />
                  </font>
               </Label>
            </children>
         </AnchorPane>
      </children>
   </GridPane>
</Scene>
